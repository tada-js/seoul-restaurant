generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}

model Restaurant {
  id         Int     @id @default(autoincrement())
  name       String? // bplcnm
  phone      String? // SITETEL
  address    String? // SITEWHLADDR
  rodaddress String? // RDNWHLADDR
  category   String? // uptaenm
  lng        String? // x
  lat        String? // y
}
